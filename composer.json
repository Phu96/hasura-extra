{
    "name": "hasura-extra/hasura-extra",
    "require": {
        "php": ">=8.0",
        "symfony/console": "^5.0",
        "symfony/filesystem": "^5.0",
        "symfony/http-client": "^5.0",
        "symfony/string": "^5.0",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symplify/monorepo-builder": "^9.4",
        "symplify/easy-coding-standard": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "Hasura\\ApiClient\\": "src/api-client/src",
            "Hasura\\Metadata\\": "src/metadata/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hasura\\ApiClient\\Tests\\": "src/api-client/tests",
            "Hasura\\Metadata\\Tests\\": "src/metadata/tests"
        }
    },
    "replace": {
        "hasura-extra/api-client": "self.version",
        "hasura-extra/metadata": "self.version"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
